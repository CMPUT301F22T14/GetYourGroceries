<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:constraintSetEnd="@id/end"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="56dp"
            android:layout_width="221dp"
            android:id="@+id/QuickAddSpinner"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:visibility="invisible"
            android:scaleX="1"
            android:scaleY="1"
            android:layout_marginBottom="90dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/home_description"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            android:layout_marginTop="44dp"
            android:scaleX="1"
            android:visibility="invisible"
            android:scaleY="1"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            android:layout_height="200dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_width="220dp"
            android:id="@+id/imageView3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp"
            android:scaleX="1"
            android:scaleY="1" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView"
            android:visibility="invisible"
            android:scaleX="1"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="172dp"
            motion:layout_constraintHorizontal_bias="0.498"
            android:scaleY="1" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/home_logoimage"
            android:id="@+id/home_description" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/floating_action_button"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/home_description"
            motion:layout_constraintVertical_bias="0.809"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            motion:layout_constraintVertical_bias="0.351"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/home_description"
            android:id="@+id/quickadd_button" />
            motion:layout_constraintTop_toBottomOf="@+id/home_description"
            android:layout_marginTop="84dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="56dp"
            android:layout_width="221dp"
            android:id="@+id/QuickAddSpinner"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:visibility="visible"
            android:scaleX="1"
            android:scaleY="1"
            android:layout_marginBottom="90dp"
            motion:layout_constraintHorizontal_bias="0.505"
            motion:layout_constraintTop_toBottomOf="@+id/textView" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/home_description"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            android:layout_marginTop="44dp"
            android:scaleX="1"
            android:visibility="visible"
            android:scaleY="1"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            android:layout_height="200dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_width="220dp"
            android:id="@+id/imageView3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp"
            android:scaleX="1"
            android:scaleY="1"
            android:visibility="visible" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView"
            android:visibility="visible"
            android:scaleX="1"
            motion:layout_constraintTop_toBottomOf="@+id/home_description"
            android:layout_marginTop="84dp" />
    </ConstraintSet>
</MotionScene>